    <div class="container">
        <h2>College Predictor Results</h2>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Institute Name</th>
                    <th>Academic Program</th>
                    <th>Category OR</th>
                    <th>Category CR</th>
                </tr>
            </thead>
            <tbody>
                <% data.forEach(entry => { %>
                    <tr>
                        <td><%= entry.Institute_Name %></td>
                        <td><%= entry.Academic_program_name %></td>
                        <td><%= entry[`${category}_${gender}_OR`] %></td>
                        <td><%= entry[`${category}_${gender}_CR`] %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
